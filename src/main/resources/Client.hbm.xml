<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ru.blogspot.feomatr.entity">
	<class name="Client" table="clients" lazy="false">
		<id name="id" column="id" type="java.lang.Long" unsaved-value="null">
			<generator class="native"/>
		</id>
		<property name="name" type="string" column="name" length="30"/>
		<property name="address" type="string" column="address" length="50"/>
		<property name="age" type="java.lang.Integer" column="age"/>
	</class>
</hibernate-mapping>
